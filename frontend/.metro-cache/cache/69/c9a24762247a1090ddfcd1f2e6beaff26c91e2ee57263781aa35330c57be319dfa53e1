ncludes(tag)) {\n          return prev.filter(t => t !== tag);\n        } else {\n          return [...prev, tag];\n        }\n      });\n    };\n    const getSelectedCount = () => {\n      return activeTab === 'diy-meals' ? selectedIngredients.size : selectedMeals.size;\n    };\n    const renderIngredientItem = ({\n      item\n    }) => {\n      const quantity = selectedIngredients.get(item._id) || 0;\n      const stepSize = item.step_size || 1;\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n        style: styles.listItemCard,\n        children: [item.images?.[0] ? /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Image.default, {\n          source: {\n            uri: item.images[0]\n          },\n          style: styles.listItemImage\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 741,\n          columnNumber: 11\n        }, this) : /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n          style: [styles.listItemImage, styles.placeholderImage],\n          children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_expoVectorIcons.Ionicons, {\n            name: \"nutrition\",\n            size: 24,\n            color: \"#ccc\"\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 744,\n            columnNumber: 13\n          }, this)\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 743,\n          columnNumber: 11\n        }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n          style: styles.listItemInfo,\n          children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n            style: styles.itemName,\n            children: item.name\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 748,\n            columnNumber: 11\n          }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n            style: styles.ite